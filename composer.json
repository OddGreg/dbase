    "keywords": ["dbase", "xbase", "dbf"],
    "homepage": "https://github.com/majkel89",
    "license": "MIT",
    "authors": [
        {
            "name": "majkel",
            "email": "maf.michal@gmail.com",
            "homepage": "https://github.com/majkel89",
